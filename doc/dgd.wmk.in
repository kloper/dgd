<attrdefault name="map" value="false" />

<contents src=".." name="README" format="text/et" />
<contents src=".." name="INSTALL" format="text/et" />
<contents src=".." name="NEWS"  format="text/et" />
<contents src=".." name="COPYING" format="text/html" />
<contents src="." name="*.html" format="text/html" namesubst="s/.html?//" />
<media src="images" name="*.jpg"  namesubst="s/\.jpe?g//" />
<media src="images" name="*.gif" namesubst="s/\.gif//" />
<media src="style" name="*.css"  namesubst="s/\.css//" />

<template name="dgd_version" format="text/html">
@DGD_SHORT_VERSION@
</template>

<template name="sfhome" format="text/html">
http://sourceforge.net/projects/dgd
</template>

<template name="cvs" format="text/html">
http://cvs.sourceforge.net/cgi-bin/viewcvs.cgi/dgd
</template>

<template name="download" format="text/html">
http://sourceforge.net/project/showfiles.php?group_id=62230
</template>

<template name="doc.code" format="text/html">
$(TOP/)/doxygen/html/index.html
</template>

<template name="page_header" format="text/html">
  <!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">  
  <html> 
  <head> 
  <meta http-equiv="expires" content="0">  
  <meta name="Generator" content="WebMake ${WebMake.Version}"> 
  <title>$[this.title]</title> 
  <link rel="stylesheet" href="$(style)" type="text/css"> 
  </head> 
  <body background="$(main_bg)">
</template>

<template name="page_footer" format="text/html">
   <div align="center">
        <table border="0" cellpadding="0" cellspacing="0" align="center">
           <tr>
              <td><br></td>
           </tr>
           <tr>
              <td align="center" class="webapptext">
              This web site was made with <a href="http://webmake.taint.org/" 
              class="webapplink">WebMake ${WebMake.Version}</a>, a web site 
              generation system.<br>
              Design, themes and other parts are taken from 
              <a href="http://www.web-app.org/cgi-bin/index.cgi" 
              class="webapplink">WebAPP v0.9.7</a>, a web portal system 
              written in Perl<br>
              </td>
	      <br> 
           </tr>
        </table>
   </div>
   </body>
   </html>
</template>

<template name="page_titlebar" format="text/html">
   <div align="center">  
      <table class="pagetable"> 
         <tr>  
	 <td>
            <table class="myheadertable"> 
	    <tr> 
               <td align="left" width="50%" rowspan="2">
	       <a href="$(home)" class="bannerlink">
		  <img src="$(dgd_web_logo)" 
		       alt="Depression Glass Debug Library"
		       border="0"
		       ${IMGSIZE}>
	       </a>
	       </td> 
               <td align="right" valign="bottom" width="50%">
	       <a href="${sfhome}" class="helps">
		  Version: ${dgd_version}</a> |
	       <a href="${download}" class="helps">
                  Download</a> | 
	       Updated ${WebMake.Time}
               </td> 
	    </tr>
	    </table>
         </td>
         </tr>
      </table>
   </div>
</template>

<template name="page_navbar" format="text/html">
   <div align="center">  
      <table border="0" cellpadding="0" cellspacing="0" width="100%" 
             class="navbar" background="$(topnav_bg)"> 
         <tr>  
	    <td width="16%" align="center">&nbsp;
               <a href="$(home)" class="nav">Home</a>&nbsp;
            </td> 
            <td width="16%" align="center">&nbsp;
	       <a href="$(faq)" class="nav">FAQ</a>&nbsp;
	    </td> 
	    <td width="16%" align="center">&nbsp;
	       <a href="$(tutorial)" class="nav">Tutorial</a>&nbsp;
	    </td> 
            <td width="16%" align="center">&nbsp;
	       <a href="$(readme)" class="nav">Documentation</a>&nbsp;
	    </td> 
	    <td width="16%" align="center">&nbsp;
	       <a href="${download}" class="nav">Downloads</a>&nbsp;
	    </td> 
	    <td width="16%" align="center">&nbsp;
	       <a href="${cvs}" class="nav">CVS</a>&nbsp;
	    </td> 
	 </tr> 
      </table>
   </div>
</template>

<template name="box" format="text/html">
   <table width="$[${item}.width?100%]">       
      <tr>                                                	       
         <td>                                             	       
            <table border="0" cellpadding="0" cellspacing="0"   	       
                   width="100%" class="menutable"               	       
                   background="$(sidebox_title_bg)">            	       
               <tr>                                             	       
                  <td>&nbsp;$[${item}.boxname?$[crumbs]]</td>          	       
               </tr>                                            	       
            </table>                                            	       
            <table border="0" cellpadding="1" cellspacing="0"               
                   width="100%" class="menubordercolor">        	       
               <tr>                                             	       
                  <td width="100%">                             	       
                  <table border="0" cellpadding="3"             	       
                         cellspacing="0" width="100%"           	       
                         class="menubackcolor">    
		     <tr>
		        <td>             	       
			${${item}} 
			</td>
		     </tr>
                  </table>                                      	       
                  </td>                                         	       
               </tr>                                            	       
            </table>                                            	       
	    <{!-- put bottom img here if needed --}>
         </td>                                                  	       
      </tr>                                                     	       
   </table>                                                     	       
</template>

<content name="box_home" format="text/html">
   <wmmeta name=boxname value="Home" />
   <wmmeta name=width value="150" />
   <table cellspacing="0" cellpadding="3">
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="$(home)">Home</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="${sfhome}">SourceForge</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="$(readme)">Documentation</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="$(tutorial)">Tutorial</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="$(faq)">FAQ</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="${cvs}">CVS</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="${download}">Download</a>      
      </tr>
   </table>
</content>

<content name="box_doc" format="text/html">
   <wmmeta name=boxname value="Documentation" />
   <wmmeta name=width value="150" />
   <table cellspacing="0" cellpadding="3">
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="$(readme)">README</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="$(install)">INSTALL</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="$(news)">NEWS</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="${doc.code}">Code Doc</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="$(tutorial)">Tutorial</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="$(faq)">FAQ</a>      
      </tr>
      <tr class="cat">
         &nbsp;&middot;&nbsp;<a href="$(todo)">Bugs/Features</a>      
      </tr>
   </table>
</content>

<content name="home" format="text/html" isroot="true" map="true">
  <wmmeta name="title" value="Depression Glass Debug Library" />
  <wmmeta name="brief" value="Home" />

  ${page_header}
  ${page_titlebar}
  ${page_navbar}
   <div align="center">
      <table class="pagetable">
         <tr>
	    <td valign="top">
               ${box: item="box_home"}
            </td>
	    <td valign="top" width="100%">
               ${box: item="NEWS"}
            </td>
	 </tr>
      </table>
   </div>
  ${page_navbar}
  ${page_footer}
</content>

<content name="readme" format="text/html" up="home" map="true">
  <wmmeta name="title" value="DGD Readme File" />
  <wmmeta name="brief" value="Readme" />
  
  ${page_header}
  ${page_titlebar}
  ${page_navbar}
   <div align="center">
      <table class="pagetable">
         <tr>
	    <td valign="top">
               ${box: item="box_home"}
               ${box: item="box_doc"}
            </td>
	    <td valign="top" width="100%">
               ${box: item="README"}
            </td>
	 </tr>
      </table>
   </div>
  ${page_navbar}
  ${page_footer}
</content>

