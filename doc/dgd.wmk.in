  ${page_navbar}
  ${page_footer}
</content>

<content name="todo" format="text/html" up="home" map="true">
  <wmmeta name="title" value="DGD To Do List" />
  <wmmeta name="brief" value="ToDo" />
  
  ${page_header}
  ${page_titlebar}
  ${page_navbar}
