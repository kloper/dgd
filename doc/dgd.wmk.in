<content name=sitemapnode map=false>
  <li>
      <a href=$(url)>$[${name}.brief]</a>
      <ul> ${list} </ul>
  </li>
</content>

<content name=sitemapleaf map=false>
  <li>
      <a href=$(url)>$[${name}.brief]</a>
  </li>
</content>

<sitemap name=mainsitemap node=sitemapnode leaf=sitemapleaf />

<template name=btop>
  	<a class="boxlink" href=${url}>$[${name}.brief]</a>
  </template>
  <template name=blevel>
  	&nbsp;:: <a class="boxlink" href=${url}>$[${name}.brief]</a> 
  </template>
  <template name=btail>
  	&nbsp;:: <a class="boxlink" href=${url}>$[${name}.brief]</a> 
  </template>
  <breadcrumbs map=sitemapname name=crumbs
  	top=btop tail=btail level=blevel />
                      

<out name="home" file="html/index.html">
   ${home}
</out>
<out name="readme" file="html/readme.html" >
   ${readme}
</out>
<out name="install" file="html/install.html" >
   ${install}
</out>
<out name="news" file="html/news.html" >
   ${news}
</out>
<out name="todo" file="html/todo.html" >
   ${todo}
</out>
<out name="faq" file="html/faq.html" >
   ${faq}
</out>
<out name="tutorial" file="html/tutorial.html" >
   ${tutorial}
</out>

<out name="sitemap" file="html/sitemap.html" >
   ${mainsitemap}
</out>

</webmake>



