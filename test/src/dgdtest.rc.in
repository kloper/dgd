 FILEFLAGS	0x0L
 FILEOS		0x4L
 FILETYPE	0x1L
 FILESUBTYPE	0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "FileDescription", "DGD test common code\0"
            VALUE "OriginalFilename", "dgdtest.dll\0"
            VALUE "CompanyName", "Dimitry Kloper\0"
            VALUE "FileVersion", "@version_string@"
            VALUE "LegalCopyright", "Copyright \251 2002-2012. Dimitry Kloper <kloper@users.sf.net>\0"
            VALUE "ProductName", "DGD\0" 
            VALUE "ProductVersion", "@version_string@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END

